    "start:dev": "ts-node-dev --poll src/server.ts",
    "start:watch": "nodemon -r src/server.ts",
    "ts.check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
