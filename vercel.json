{
  "version": 2,
  "builds": [
    {
      "src": "dist/src/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/src/**"] }
    }
  ],
  "routes": [
    {
      "src": "dist/public/(.*)",
      "dest": "dist/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/src/server.js"
    }
  ],
  "public": true
}
